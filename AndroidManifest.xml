<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.siddiqsami.intrepido" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Register"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Signin"
            android:label="@string/title_activity_signin" >
            <intent-filter>
                <action android:name="com.example.siddiqsami.Intrepido.Signin"/>

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Signup"
            android:label="@string/title_activity_signup" >
            <intent-filter>
                <action android:name="com.example.siddiqsami.Intrepido.Signup"/>

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Start"
            android:label="@string/title_activity_start" >
        </activity>
        <activity
            android:name=".Emergency"
            android:label="@string/title_activity_emergency" >
        </activity>
    </application>

</manifest>
